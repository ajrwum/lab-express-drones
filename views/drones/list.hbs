<h2>Available drones</h2>

<div id="drones">
  {{#each allDrones}}
  <div class="card">
    <h3>{{this.name}}</h3>
    <p>{{this.propellers}} propeller(s)</p>
    <p>Max speed: {{this.maxSpeed}} knots</p>
    <a href="/drones/{{this._id}}/edit">Update</a>
    <a href="/drones/{{this._id}}/delete">Delete</a>
  </div>
  {{/each}}
</div>